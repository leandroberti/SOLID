<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="CustomerEmail" value="customer@domain.com"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network" from="noreply@solidsample.com">
        <network host="smtp.gmail.com" port="587" userName="myemail@gmail.com" password="mypassword" />
      </smtp>
    </mailSettings>
  </system.net>
</configuration>